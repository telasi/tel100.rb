<%= editor_for @type do |e|
  e.required_combo_field 'category', collection: document_response_types, select2: false, i18n: 'typename'
  e.required_text_field 'ordering', autofocus: true
  e.required_text_field 'name_ka'
  e.text_field 'name_ru'
  e.text_field 'name_en'
  e.submit 'მონაცემების შენახვა'
  e.button_action admin_response_types_url, label: ' გაუქმება', icon: 'remove'
end %>