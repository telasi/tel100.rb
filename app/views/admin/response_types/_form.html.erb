<%= editor_for @type do |e|
  e.required_combo_field 'typekey', collection: document_response_types, select2: false, label: 'ტიპი'
  e.required_text_field 'ordering', label: 'დალაგება', autofocus: true
  e.required_text_field 'name_ka', label: 'დასახელება (KA)'
  e.text_field 'name_ru', label: 'დასახელება (RU)'
